        <script type="text/javascript">
                $(function () {
                  $('#accept_arrow').click(function(){
                    var offset = $('.section-bring-it').offset().top - $('.nav-bar').height();
                    $('html,body').animate({
                        scrollTop: offset
                    }, 1000);
                    return false;

                  });
                    Dash.initAll(null, null, $('#accept_title_holder'));
                    //var animDelay = 15;
                    //$('.accept-title').find('div').each(function (index, ele) {
                    //    setTimeout(function (element) {
                    //        $(element).css({ 'display': 'inline-block' }).addClass('animated fadeIn');

                    //    }, animDelay, ele);
                    //    animDelay += 1000;
                    //});
                    var animDelay = 15;
                    $('.accept-icon-container').each(function (index, ele) {
                        $(this).css({ '-webkit-animation-delay': animDelay + 'ms', 'animation-delay': animDelay + 'ms' });
                        animDelay += 15;
                    });

                    $('.accept-icon-container').onScreen({
                        tolerance: 0,
                        throttle: 50,
                        doIn: function () {
                            $(this).addClass('moveUp');
                        }
                    });


                    $('#apply_link').click(function () {
                        if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
                            var target = $(this.hash);
                            target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
                            if (target.length) {
                                $('html,body').animate({
                                    scrollTop: target.offset().top
                                }, 1000);
                                return false;
                            }
                        }
                    });


                });
            </script>

