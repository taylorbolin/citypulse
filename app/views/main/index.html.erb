<script type="text/javascript">
    $(function () {
        var section = $('.section');
        var carouselPager = $('.carousel-pager');
        var carouselContainer = $('#carousel_container');
        var sectionCities = $('.section-cities');
        var sectionVenueVibe = $('.section-venue-vibe');
        var sectionCityContainer = $('.section-city-container');

        setTimeout(function () {
            $(carouselContainer.find('.section-text')[currentPage]).addClass('animated fadeInUp')
        }, 300);

        Dash.initAll(initSectionListener, initScroll, $('.section-what-is'));

        function initOnScreen() {
            sectionVenueVibe.onScreen({
                container: window,
                direction: 'vertical',
                doIn: function () {
                    sectionVenueVibe.addClass('zoomInImage');
                },
                tolerance: 100,
                throttle: 50,
                debug: false
            });
            var delay = 300;
            sectionCities.onScreen({
                container: window,
                direction: 'vertical',
                doIn: function () {
                    sectionCityContainer.each(function () {
                        setTimeout(function (element) {
                            $(element).addClass('animated fadeInUpSmall');
                        }, delay, $(this));
                        delay += 200;
                    });
                },
                tolerance: 100,
                throttle: 50,
                debug: false
            });
        }

        function initSectionListener() {
            initCarousel();
            carouselContainer.each(function (index, element) {
                $(element).find('.slick-list').height(section.height());
                $(element).find('.slick-track').height(section.height());
            });
            initOnScreen();
        }

        function initScroll(scrollPos, headerHeight) {
            var pagerContainer = carouselPager.position().top;

            if (scrollPos > 0 && scrollPos > pagerContainer - (headerHeight * 3)) {
                if (!carouselPager.hasClass('fadeOut')) {
                    carouselPager.removeClass('animated fadeIn').addClass('animated fadeOut');
                    $(carouselContainer.find('.section-text')[currentPage]).removeClass('animated fadeIn').addClass('animated fadeOut');
                }
            } else {
                if (carouselPager.hasClass('fadeOut')) {
                    carouselPager.removeClass('animated fadeOut').addClass('animated fadeIn');
                    $(carouselContainer.find('.section-text')[currentPage]).removeClass('animated fadeOut').addClass('animated fadeIn');
                }
            }
        }

        var pager;
        var currentPage = 0;

        function initCarousel() {
            if (!pager) {
                pager = carouselContainer.slick({
                    infinite: true,
                    speed: 700,
                    arrows: false,
                    slidesToShow: 1,
                    fade: true,
                    cssEase: 'ease-out',
                    onInit: function () {
                        // This runs after the slickgrid is first initialized.
                        this.$list.css('height', this.$slider.parents('.inner').outerHeight(true))

                    }
                });
                carouselContainer.each(function (index, element) {
                    $(element).find('.slick-list').height(section.height());
                    $(element).find('.slick-track').height(section.height());
                });
                carouselPager.click(function () {
                    pager.slick($(this).attr('data-dash-pager'));
                });
                var input = $('#download_container').detach();
                $(carouselContainer.find('.header')[currentPage]).find('.download-icons-container').append(input);
                $(pager).on('afterChange', function (event, slick, currentSlide, nextSlide) {
                    currentPage = currentSlide;
                    var input = $('#download_container').detach();
                    $(carouselContainer.find('.header')[currentSlide]).find('.download-icons-container').append(input);
                    carouselContainer.find('.header').find('.section-text').removeClass('animated fadeInUp');
                    $(carouselContainer.find('.header')[currentSlide]).find('.section-text').addClass('animated fadeInUp');
                });
            }
        }
    });
</script>

